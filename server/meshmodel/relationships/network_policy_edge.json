{
    "apiVersion": "core.meshery.io/v1alpha1",
    "kind": "Edge",
    "metadata": {
      "description": "A relationship that acts as a firewall for network communication between pods"
    },
      "model":{
          "name": "core",
          "version": "v1.0.0",
          "displayName": "Core",
          "category": {
        "name":"Cloud Native Application",
        "metadata": null
      },
      "metadata": {}
    },
    "subType": "Firewall",
    "selectors": {
      "allow":{
        "from": [
            {
                "kind": "Pod",
                "model": "Kubernetes"
            }
        ],
        "to": [
            {
                "kind": "Pod",
                "model": "Kubernetes"
            }
        ]
      }
    }
  }